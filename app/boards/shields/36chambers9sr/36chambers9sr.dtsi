#include <dt-bindings/zmk/matrix_transform.h>

/ {
	chosen {
		zmk,kscan = &kscan0;
		zmk,matrix_transform = &default_transform;
	};

	default_transform: keymap_transform_0 {
		compatible = "zmk,matrix-transform";
		columns = <10>;
        
		rows = <6>;
// | SW1  | SW2  | SW3  | SW4  | SW5  |   | SW5  | SW4  | SW3  | SW2  | SW1  |
// | SW6  | SW7  | SW8  | SW9  | SW10 |   | SW10 | SW9  | SW8  | SW7  | SW6  |
// | SW11 | SW12 | SW13 | SW14 | SW15 |   | SW15 | SW14 | SW13 | SW12 | SW11 |
// | SW16 |      |      | SW17 | SW18 |   | SW19 | SW19 | SW18 |      | SW16 |
// | U3A  | U3CT | U3C  | U3B  | U3D  |   | SW24 | SW23 | SW22 | SW21 | SW20 |
		map = <
RC(0,0) RC(0,1) RC(0,2) RC(0,3) RC(0,4)           RC(0,6) RC(0,7) RC(0,8) RC(0,9) 
RC(1,0) RC(1,1) RC(1,2) RC(1,3) RC(1,4)           RC(1,6) RC(1,7) RC(1,8) RC(1,9) 
RC(2,0) RC(2,1) RC(2,2) RC(2,3) RC(2,4)           RC(2,6) RC(2,7) RC(2,8) RC(2,9) 
RC(3,0)         RC(3,2) RC(3,3) RC(3,4)           RC(3,6) RC(3,7)         RC(3,9)         
RC(4,0) RC(4,1) RC(4,2) RC(4,3) RC(4,4) RC(4,5)     RC(4,6) RC(4,7) RC(4,8) RC(4,9) 
RC(5,7)
		>;
	};
